@{ 
  Layout = "default";
  Title = "Module";
}

<div class="row">
  <div class="span1"></div>
  <div class="span10" id="main">
    <h1>@Model.Type.Name</h1>

    <div class="xmldoc">
      @Model.Type.Comment.FullText
    </div>

    @RenderPart("members", new {
        Header = "Union Cases",
        TableHeader = "Union Cases",
        Members = Model.Type.UnionCases
    })

    @RenderPart("members", new {
        Header = "Record Fields",
        TableHeader = "Record Fields",
        Members = Model.Type.RecordFields
    })

    @RenderPart("members", new { 
        Header = "Constructors",
        TableHeader = "Constructors",
        Members = Model.Type.Constructors
    })

    @RenderPart("members", new { 
        Header = "Instance members",
        TableHeader = "Instance members",
        Members = Model.Type.InstanceMembers
    })

    @RenderPart("members", new { 
        Header = "Static members",
        TableHeader = "Static members",
        Members = Model.Type.StaticMembers
    })

  </div>
  <div class="span1"></div>
</div>
